{
  "editor.fontFamily": "'OperatorMonoSSmLig-Book',Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "relative",
  "workbench.colorTheme": "Night Owl Light",
  "workbench.iconTheme": "material-icon-theme",
  "git.enableSmartCommit": true,
  "git.postCommitCommand": "sync",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "git.confirmSync": false,
  "git.autofetch": true,
  "files.autoSave": "onFocusChange",
  "editor.minimap.showSlider": "always",
  "editor.tabSize": 2,
  "files.associations": {
    ".inputrc": "shellscript"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.singleQuote": true,
  "editor.renderWhitespace": "all",
  "workbench.preferredDarkColorTheme": "Night Owl",
  "editor.smoothScrolling": true,
  "editor.fontLigatures": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.scrollBeyondLastLine": false,
  "editor.minimap.enabled": false,
  "explorer.compactFolders": false,
  "eslint.validate": ["javascript", "vue"],
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "workbench.tree.indent": 12,
  "workbench.startupEditor": "newUntitledFile",
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.styleInitialIndent": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 0,
  "prettier.jsxSingleQuote": true,
  "editor.formatOnSave": true,
  "tailwindCSS.includeLanguages": {
    "vue": "html"
  },
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/maxpiano/.vscode/init.vim",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "editor.fontWeight": "200",
  "terminal.integrated.fontWeightBold": "normal",
  "customizeUI.stylesheet": {
    ".label-name": "letter-spacing: 0.25px !important"
  },
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.preferredHighContrastColorTheme": "Activate UMBRA protocol",
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    }
  ]
}
